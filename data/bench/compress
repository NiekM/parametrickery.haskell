compress : Eq a => {xs : List a} -> List a
compress [] = []
compress [0] = [0]
compress [1] = [1]
compress [0,0] = [0]
compress [1,1] = [1]
compress [2,0] = [2,0]
compress [1,0,0] = [1,0]
compress [0,1,1] = [0,1]
compress [2,1,0,0] = [2,1,0]
compress [2,2,1,0,0] = [2,1,0]
compress [2,2,0] = [2,0]
compress [2,2,2,0] = [2,0]
compress [1,2,2,2,0] = [1,2,0]