size : {t : Tree a b} -> Int
size Leaf = 0
size (Node (Leaf, 2, Leaf)) = 1
size (Node (Node (Leaf, 1, Leaf), 2, Leaf)) = 2
size (Node (Leaf, 2, Node (Leaf, 3, Leaf))) = 2
size (Node (Node (Leaf, 1, Leaf), 2, Node (Leaf, 3, Leaf))) = 3
size (Node (Leaf, 2, Node (Leaf, 3, Node (Leaf, 4, Leaf)))) = 3
size (Node (Leaf, 2, Node (Node (Leaf, 0, Leaf), 3, Leaf))) = 3
size (Node (Node (Leaf, 3, Node (Leaf, 4, Leaf)), 2, Leaf)) = 3
size (Node (Node (Node (Leaf, 0, Leaf), 3, Leaf), 2, Leaf)) = 3
size (Node (Node (Node (Leaf, 4, Leaf), 2, Leaf), 1, Node (Leaf, 3, Leaf))) = 4
size (Node (Node (Node (Leaf, 4, Node (Leaf, 5, Leaf)), 2, Leaf), 1, Node (Leaf, 3, Leaf))) = 5
size (Node (Node (Node (Leaf, 4, Leaf), 2, Node (Leaf, 5, Leaf)), 1, Node (Leaf, 3, Leaf))) = 5
size (Node (Node (Node (Leaf, 4, Leaf), 2, Node (Leaf, 5, Leaf)), 1, Node (Node (Leaf, 6, Leaf), 3, Node (Leaf, 7, Leaf)))) = 7
size (Node (Node (Node (Leaf, 4, Node (Leaf, 5, Leaf)), 2, Leaf), 1, Node (Node (Leaf, 6, Leaf), 3, Node (Leaf, 7, Leaf)))) = 7